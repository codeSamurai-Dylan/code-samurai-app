<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>
                Actions
            </th>
            <th>Video Game</th>
            <th>Rating</th>
            <th>Publisher</th>
            <th>Genre</th>
            <th>URL</th>
            <th>Added By</th>
            <th>Likes</th>
        </tr>

        <% items.forEach(function(item){  %>

        <tr>
            <td>
                <i class="material-icons">thumb_up</i>
                <i class="material-icons">thumb_down</i>
            </td>

            <td>
                <a><%=item.nameofvideogame%></a>
            </td>
            <td>
                <%=item.rating%>
            </td>
            <td>
                <%=item.publisher%>
            </td>
            <td>
                <%=item.genre%>
            </td>
            <td>
                <%=item.url%>
            </td>


        </tr>



        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
